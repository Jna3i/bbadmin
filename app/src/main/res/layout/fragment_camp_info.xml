<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pifss.bbadmin.CampInfo">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/frgCampInfo_txtCampNameID"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center_horizontal"
            android:text="Campaign Name" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/frgCampInfo_mapViewID"
            android:layout_width="match_parent"
            android:layout_height="208dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_txtCampNameID" />

        <TextView
            android:id="@+id/frgCampInfo_txtDateFromID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView13"
            android:gravity="center_horizontal"
            android:text="TextView" />

        <TextView
            android:id="@+id/frgCampInfo_locationID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_mapViewID"
            android:layout_marginTop="46dp"
            android:gravity="center_horizontal"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_locationID"
            android:layout_marginTop="25dp"
            android:gravity="center_horizontal"
            android:text="From" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_txtDateFromID"
            android:layout_marginTop="15dp"
            android:gravity="center_horizontal"
            android:text="To" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:gravity="center_horizontal"
            android:text="Location" />

        <TextView
            android:id="@+id/frgCampInfo_txtDateToID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView14"
            android:gravity="center_horizontal"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_txtDateToID"
            android:layout_marginTop="13dp"
            android:gravity="center_horizontal"
            android:text="Description" />

        <TextView
            android:id="@+id/frgCampInfo_txtDescriptionID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frgCampInfo_txtDateToID"
            android:layout_marginTop="30dp"
            android:gravity="center_horizontal"
            android:text="TextView" />

        <Button
            android:id="@+id/frgCampInfo_btnEditID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="39dp"
            android:text="Edit" />
    </RelativeLayout>


</FrameLayout>
